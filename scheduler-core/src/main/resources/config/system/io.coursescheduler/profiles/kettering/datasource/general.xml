<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<entry key="file-uri-template">${source.protocol}://${source.path}/${source.file}</entry>
	<entry key="file-path-template">${dir.data}/${source.file}</entry>
	<entry key="file-save-copy">true</entry>
	<entry key="file-copy-path-template">${dir.tmp}/${source.file}</entry>
</properties>